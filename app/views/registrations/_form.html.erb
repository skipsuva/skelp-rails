<%= form_tag registrations_path do %>
  <div class="">
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <div class="">
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </div>
  <div class="">
    <%= label_tag :password %>
    <%= text_field_tag :password %>
  </div>
  <div class="">
    <%= label_tag :password_confirmation %>
    <%= text_field_tag :password_confirmation %>
  </div>
  <div class="">
    <%= submit_tag "Sign Up, Bro" %>
  </div>
<% end %>
